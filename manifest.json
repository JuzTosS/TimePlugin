{
  "manifest_version": 2,

  "name": "Zodiac work time helper",
  "description": "This extension shows time when you should go home.",
  "version": "0.1",
  "icons": { "64": "logo.png" },
  "content_scripts": [
    {
      "matches": ["http://attendance.developonbox.ru/*"],
      "run_at": "document_end",
      "all_frames": false,
      "js": ["jquery-2.1.4.js", "date.js", "TimePlugin.js"]
    }
  ]
}